[global]
ioengine=dfs
pool=tank
cont=bucket
filename_format=fio-test.$jobnum

cpus_allowed_policy=split
group_reporting=1
time_based=0
percentile_list=99.0:99.9:99.99:99.999:99.9999:100
disable_slat=1
disable_clat=1

bs=1M
size=30G
iodepth=150
numjobs=50

[daos-seqwrite]
rw=write

[daos-seqread]
stonewall
rw=read

[trim]
stonewall
rw=trim
